{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "white": "#FFFFFF",
    "tan": "#CC3802",
    "teal": "#047E84",
    "plum": "#9A348E",
    "blush": "#DA627D",
    "salmon": "#FCA17D",
    "sky": "#86BBD8",
    "teal_blue": "#33658A"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "accordion",
          "leading_diamond": "\ue0b6",
          "foreground": "p:white",
          "background": "p:tan",
          "template": "{{ if .Env.PNPPSHOST }} \uf8c5 {{ .Env.PNPPSHOST }} {{ end }}"
        },
        {
          "type": "text",
          "style": "powerline",
          "foreground": "p:white",
          "background": "p:teal",
          "powerline_symbol": "\ue0b0",
          "template": "{{ if .Env.PNPPSSITE }} \uf2dd {{ .Env.PNPPSSITE }}{{ end }}"
        },
        {
          "type": "text",
          "style": "diamond",
          "trailing_diamond": "\ue0b4",
          "foreground": "p:white",
          "background": "p:teal",
          "template": "{{ if .Env.PNPPSSITE }}\u00A0{{ end }}"
        }
      ],
      "type": "rprompt"
    },
    {
      "alignment": "left",
      "segments": [
        {
          "background": "p:plum",
          "foreground": "p:white",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": "Oheyo ",
          "type": "session"
        },
        {
          "background": "p:blush",
          "foreground": "p:white",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "folder_icon": "ðŸ“‚",
            "folder_separator_icon": " / ",
            "style": "agnoster_short",
            "home_icon": "\ue617 ",
            "max_depth": 1
          },
          "style": "powerline",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "p:salmon",
          "foreground": "p:white",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "branch_icon": "",
            "fetch_stash_count": true,
            "fetch_status": false,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " \u279c ({{ url .UpstreamIcon .UpstreamURL }}{{ .HEAD }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }}) ",
        "type": "git" 
        },
        {
          "background": "p:sky",
          "foreground": "p:white",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "type": "node"
        },
        {
            "type": "unity",
            "style": "powerline",
            "powerline_symbol": "\ue0b0",
            "foreground": "#111111",
            "background": "#ffffff",
            "template": "\ue721 {{ .UnityVersion }}{{ if .CSharpVersion }}{{ .CSharpVersion }} {{ end }}",
            "properties": {
              "http_timeout": 2000
            }
        },
        {
          "background": "p:teal_blue",
          "foreground": "p:white",
          "properties": {
            "time_format": "15:04"
          },
          "style": "diamond",
          "template": " {{ .CurrentDate | date .Format }} ",
          "trailing_diamond": "\ue0b0",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "type": "executiontime",
          "style": "diamond",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0b0",

          "foreground": "#ffffff",
          "background": "#8800dd",
          "template": " <#fefefe>î®¢</> {{ .FormattedMs }} ",
          "properties": {
            "threshold": 1,
            "style": "austin",
            "always_enabled": false
          }
        },
        {
          "type": "sysinfo",
          "foreground": "#ffffff",
          "background": "#8f43f3",
          "template": " î‰¦ {{ round .PhysicalPercentUsed .Precision }}% ",
          "properties": {
            "precision": 2
          },
          "style": "diamond",
          "trailing_diamond": "\ue0b4"

        }
      ],
      "type": "prompt"
    },

    {
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "background": "transparent",
          "template": "\nðŸ‘¾ ï„œ  î¬¬"          
        }
      ],
      "type": "prompt"
    }
    
  ],
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#FEF5ED",
    "template": "\ue285 "
  },
  "final_space": true,
  "version": 2
}
